{
    "multipart": [
        {   "when": { "thick": "false" },
            "apply": { "model": "minecraftfuture:bamboo_thin"}
        },
        {
            "when": { "thick": "true"},
            "apply": { "model": "minecraftfuture:bamboo_thick"}
        },
        {
            "when": { "leaves": "small"},
            "apply": { "model": "minecraftfuture:bamboo_leaves_0"}
        },
        {
            "when": { "leaves": "large"},
            "apply": { "model": "minecraftfuture:bamboo_leaves_1"}
        }
    ]
}